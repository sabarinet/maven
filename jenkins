node('built-in') 
{
 stage('Continuous Download') 
 {
 git 'https://github.com/ajdevops116/maven.git'
 }
 stage('Continuous Build') 
 {
 sh 'mvn package' 
 }
 stage('Continuous Deployment') 
 {
 
sh ' scp /home/ubuntu/.jenkins/workspace/ScriptedPipeline/webapp/target/webapp.war 
ubuntu@172.31.43.170:/var/lib/tomcat9/webapps/qaenv.war
'
 }
 stage('Continuous Testing') 
 {
 sh 'echo "Testing Passed"'
 }
 stage('Continuous Delivery') 
 {
 
sh ' scp /home/ubuntu/.jenkins/workspace/ScriptedPipeline/webapp/target/webapp.war 
ubuntu@172.31.38.65:/var/lib/tomcat8/webapps/prodenv.war'
 }
}
